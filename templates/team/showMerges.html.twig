{% extends 'base.html.twig' %}

{% block title %}Merges of the project{% endblock %}

{% block body %}

        <table class="table table-striped table-bordered">
            <thead class="thead-dark">
            <tr>
                <th scope="row">Nom</th>
                <th scope="row">Description</th>
                <th scope="row">Date de modification</th>
            </tr>
            </thead>
            <tbody>
            {% for item in merges %}
                <tr>
                    <td>{{ item.title }}</td>
                    <td>{{ item.description }}</td>
                    <td>{{ item.updated_at | date('m/d/Y h:m') }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>


    </div>


{% endblock %}
